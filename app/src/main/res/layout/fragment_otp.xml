<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".login">


    <ImageView
        android:id="@+id/verifybutton"
        android:layout_width="348dp"
        android:layout_height="62dp"
        android:layout_marginTop="639dp"
        android:layout_marginBottom="145dp"
        android:contentDescription="@string/desc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/black_button" />

    <TextView
        android:id="@+id/warning3"
        android:layout_width="202dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:fontFamily="@font/light"
        android:text="*please enter correct code"
        android:textColor="@color/red"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.979"
        app:layout_constraintStart_toStartOf="@+id/verifybutton"
        app:layout_constraintTop_toBottomOf="@+id/verifybutton"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/getotpmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/regular"
        android:text="Verify"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/verifybutton"
        app:layout_constraintEnd_toEndOf="@+id/verifybutton"
        app:layout_constraintStart_toStartOf="@+id/verifybutton"
        app:layout_constraintTop_toTopOf="@+id/verifybutton"
        app:layout_constraintVertical_bias="0.533"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/otpmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="374dp"
        android:layout_marginBottom="441dp"
        android:fontFamily="@font/bold"
        android:text="OTP Verification"
        android:textColor="@color/colorPrimary"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/nootpmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/regular"
        android:text="Don't get the OTP ?"
        android:textColor="@color/mediumgray"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/verifybutton"
        app:layout_constraintEnd_toEndOf="@+id/verifybutton"
        app:layout_constraintHorizontal_bias="0.302"
        app:layout_constraintStart_toStartOf="@+id/verifybutton"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/noentermsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:fontFamily="@font/bold"
        android:text=" RESENT"
        android:textColor="@color/red"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/nootpmsg"
        app:layout_constraintStart_toEndOf="@+id/nootpmsg"
        app:layout_constraintTop_toTopOf="@+id/nootpmsg"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/msg1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/regular"
        android:text="Enter the OTP sent to"
        android:textColor="@color/hardgray"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/otpmsg"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/phoneno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:fontFamily="@font/regular"
        android:text=" +91- "
        android:textColor="@color/colorPrimary"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/msg1"
        app:layout_constraintStart_toEndOf="@+id/msg1"
        app:layout_constraintTop_toTopOf="@+id/msg1"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/backarrow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="38.5dp"
        android:layout_marginTop="82.5dp"
        android:layout_marginRight="353.3dp"
        android:layout_marginBottom="751.4dp"
        android:contentDescription="@string/desc"
        android:src="@drawable/ic_back_arrow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="38.5dp"
        android:layout_marginEnd="353.3dp" />
    <ImageView
        android:id="@+id/line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="585.5dp"
        android:layout_marginBottom="258.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_name_line"
        tools:ignore="ContentDescription,VectorDrawableCompat" />
    <EditText
        android:id="@+id/otpno"
        android:layout_width="250dp"
        android:layout_height="27dp"
        android:background="@null"
        android:ems="10"
        android:maxLength="6"
        android:letterSpacing="1.2"
        android:fontFamily="@font/bold"
        android:inputType="phone"
        android:gravity="center"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/line"
        app:layout_constraintEnd_toEndOf="@+id/line"
        app:layout_constraintStart_toStartOf="@+id/line"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:targetApi="lollipop"
        android:autofillHints="" />


</androidx.constraintlayout.widget.ConstraintLayout>
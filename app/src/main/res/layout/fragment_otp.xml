<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".login">


    <ImageView
        android:id="@+id/getotpbutton"
        android:layout_width="348dp"
        android:layout_height="62dp"
        android:layout_marginTop="639dp"
        android:layout_marginBottom="145dp"
        android:contentDescription="@string/desc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/black_button" />

    <TextView
        android:id="@+id/getotpmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/regular"
        android:text="Verify"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/getotpbutton"
        app:layout_constraintEnd_toEndOf="@+id/getotpbutton"
        app:layout_constraintStart_toStartOf="@+id/getotpbutton"
        app:layout_constraintTop_toTopOf="@+id/getotpbutton"
        app:layout_constraintVertical_bias="0.533"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/otpmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="374dp"
        android:layout_marginBottom="441dp"
        android:fontFamily="@font/bold"
        android:text="OTP Verification"
        android:textColor="@color/colorPrimary"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/nootpmsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/regular"
        android:text="Don't get the OTP ?"
        android:textColor="@color/mediumgray"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/getotpbutton"
        app:layout_constraintEnd_toEndOf="@+id/getotpbutton"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="@+id/getotpbutton"
        app:layout_constraintTop_toBottomOf="@+id/smallline2"
        app:layout_constraintVertical_bias="0.769"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/noentermsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:fontFamily="@font/bold"
        android:text=" RESENT"
        android:textColor="@color/red"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/nootpmsg"
        app:layout_constraintStart_toEndOf="@+id/nootpmsg"
        app:layout_constraintTop_toTopOf="@+id/nootpmsg"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/msg1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/regular"
        android:text="Enter the OTP sent to"
        android:textColor="@color/hardgray"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/otpmsg"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/msg2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:fontFamily="@font/regular"
        android:text=" +91- "
        android:textColor="@color/colorPrimary"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/msg1"
        app:layout_constraintStart_toEndOf="@+id/msg1"
        app:layout_constraintTop_toTopOf="@+id/msg1"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/backarrow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="38.5dp"
        android:layout_marginTop="82.5dp"
        android:layout_marginRight="353.3dp"
        android:layout_marginBottom="751.4dp"
        android:contentDescription="@string/desc"
        android:src="@drawable/ic_back_arrow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="38.5dp"
        android:layout_marginEnd="353.3dp" />

    <ImageView
        android:id="@+id/smallline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="551.5dp"
        android:layout_marginBottom="295.5dp"
        android:contentDescription="@string/desc"
        android:src="@drawable/ic_small_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/smallline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/smallline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="551.5dp"
        android:layout_marginBottom="295.5dp"
        android:src="@drawable/ic_small_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/smallline3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/smallline1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:contentDescription="@string/desc" />

    <ImageView
        android:id="@+id/smallline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="551.5dp"
        android:layout_marginBottom="295.5dp"
        android:src="@drawable/ic_small_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/smallline4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/smallline2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:contentDescription="@string/desc" />

    <ImageView
        android:id="@+id/smallline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="551.5dp"
        android:layout_marginBottom="295.5dp"
        android:src="@drawable/ic_small_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/smallline3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:contentDescription="@string/desc" />

    <EditText
        android:id="@+id/no1"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="2dp"
        android:autofillHints=""
        android:background="@null"
        android:ems="10"
        android:text="5"
        android:gravity="center"
        android:fontFamily="@font/bold"
        android:inputType="phone"
        android:textColor="@color/colorPrimary"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/smallline1"
        app:layout_constraintEnd_toEndOf="@+id/smallline1"
        app:layout_constraintStart_toStartOf="@+id/smallline1"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints" />
    <EditText
        android:id="@+id/no2"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="2dp"
        android:autofillHints=""
        android:background="@null"
        android:ems="10"
        android:text="5"
        android:gravity="center"
        android:fontFamily="@font/bold"
        android:inputType="phone"
        android:textColor="@color/colorPrimary"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/smallline2"
        app:layout_constraintEnd_toEndOf="@+id/smallline2"
        app:layout_constraintStart_toStartOf="@+id/smallline2"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints" />
    <EditText
        android:id="@+id/no3"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="2dp"
        android:autofillHints=""
        android:background="@null"
        android:ems="10"
        android:text="5"
        android:gravity="center"
        android:fontFamily="@font/bold"
        android:inputType="phone"
        android:textColor="@color/colorPrimary"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/smallline3"
        app:layout_constraintEnd_toEndOf="@+id/smallline3"
        app:layout_constraintStart_toStartOf="@+id/smallline3"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints" />
    <EditText
        android:id="@+id/no4"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="2dp"
        android:autofillHints=""
        android:background="@null"
        android:ems="10"
        android:text="5"
        android:gravity="center"
        android:fontFamily="@font/bold"
        android:inputType="phone"
        android:textColor="@color/colorPrimary"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/smallline4"
        app:layout_constraintEnd_toEndOf="@+id/smallline4"
        app:layout_constraintStart_toStartOf="@+id/smallline4"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>